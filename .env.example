# MarketNarrative 环境变量配置模板
# 复制此文件为 .env 并填写实际值

# Flask密钥（生产环境必需）
# 生成方式: python3 -c "import secrets; print(secrets.token_hex(32))"
SECRET_KEY=your-secret-key-here-change-this-in-production

# 应用访问密码（生产环境必需）
APP_PASSWORD=your-secure-password-here

# 数据库连接（生产环境必需，Railway会自动注入）
DATABASE_URL=

# 应用端口（可选，默认5001）
PORT=5001

# Flask调试模式（开发环境可设为True，生产环境必须为False）
FLASK_DEBUG=False
